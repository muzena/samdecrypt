<!--?xml version="1.0"?-->
<settings>
   
  <!-- for detailed info about the settings see http://webgrabplus.com/node/30 -->
   
  <filename>../guide-viptv.xml</filename>
  <mode>n</mode>
  <postprocess run="n" grab="y">mdb</postprocess>
  <logging>on</logging>
  <retry time-out="7">4</retry>
  <timespan>10</timespan>
  <update>i</update>
 
  <!-- replace the next dummy channel entry with the channels you want -->
  <!-- see http://webgrabplus.com/node/94 for the available sites/channels -->
    <!-- HRVATSKI PROGRAMI -->
    <channel update="i" site="bnet.hr" site_id="957" xmltv_id="HTV 1">HTV 1</channel>
    <channel offset="1" same_as="HTV 1" xmltv_id="HRT 1">HRT 1</channel>
    <channel update="i" site="bnet.hr" site_id="958" xmltv_id="HTV 2">HTV 2</channel>
    <channel offset="1" same_as="HTV 2" xmltv_id="HRT 2">HRT 2</channel>

</settings>
