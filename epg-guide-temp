#!/bin/bash
# Script for EPG grab from WebGrab+Plus  http://www.webgrabplus.com/documentation
set -x

function git_clone()
{
git clone https://github.com/muzena/samdecrypt.git -b libepg
wait $!
cd ./samdecrypt
./update-epg-guide <<EOF
EOF
}


git_clone
